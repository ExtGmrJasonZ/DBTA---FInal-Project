            <font>
                <Font name="System Bold" size="12.0" />
            </font></Button>
        <Button fx:id="btnSignOut" layoutX="12.0" layoutY="380.0" mnemonicParsing="false" onMouseClicked="#signOutButtonAction" text="SIGN OUT">
            <font>
                <Font name="System Bold Italic" size="12.0" />
            </font></Button>
        <Button fx:id="btnBack" layoutX="13.0" layoutY="350.0" mnemonicParsing="false" onMouseClicked="#backButtonAction" text="BACK">
            <font>
                <Font name="System Bold Italic" size="12.0" />
            </font></Button>
        <Label layoutX="41.0" layoutY="14.0" text="PRODUCT" textFill="#4400ff">
            <font>
                <Font name="System Bold" size="23.0" />
            </font>
        </Label>

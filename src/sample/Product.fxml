        <Button fx:id="deleteData" layoutX="100.0" layoutY="270.0" mnemonicParsing="false" onAction="#deleteData" prefHeight="25.0" prefWidth="91.0" text="Delete">
            <font>
                <Font name="System Bold" size="12.0" />
            </font></Button>
        <TextField fx:id="id" layoutX="85.0" layoutY="60.0" prefHeight="25.0" prefWidth="111.0" />
        <Text layoutX="59.0" layoutY="77.0" strokeType="OUTSIDE" strokeWidth="0.0" text="ID:" />
        <TextField fx:id="name" layoutX="85.0" layoutY="95.0" prefHeight="25.0" prefWidth="111.0" />
        <Text layoutX="39.0" layoutY="112.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Name:" />
        <TextField fx:id="quantity" layoutX="85.0" layoutY="130.0" prefHeight="25.0" prefWidth="111.0" />
        <Text layoutX="25.0" layoutY="146.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Quantity:" />
        <TextField fx:id="price" layoutX="85.0" layoutY="165.0" prefHeight="25.0" prefWidth="111.0" />
        <Text layoutX="47.0" layoutY="181.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Price:" wrappingWidth="48.18163827061653" />

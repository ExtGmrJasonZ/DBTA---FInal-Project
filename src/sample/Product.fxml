                <TableColumn fx:id="product_id" prefWidth="38.333343505859375" text="Id" />
                <TableColumn fx:id="product_name" prefWidth="120.66668701171875" text="Name" />
                <TableColumn fx:id="product_quantity" prefWidth="60.0" text="Quantity" />
                <TableColumn fx:id="product_price" prefWidth="100.0" text="Price" />
            </columns>
        </TableView>
        <Button fx:id="btn_generate" layoutX="100.0" layoutY="300.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="25.0" prefWidth="100.0" text="Refresh" />
